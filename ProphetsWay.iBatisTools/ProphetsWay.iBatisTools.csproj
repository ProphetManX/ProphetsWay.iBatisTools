<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45;net451;net452;net46;net461;net471;netstandard2.0;netcoreapp2.2;netcoreapp2.1;netcoreapp2;</TargetFrameworks>
    <RootNamespace>ProphetsWay.iBatisTools</RootNamespace>
    <AssemblyName>ProphetsWay.iBatisTools</AssemblyName>
    <ApplicationIcon />
    <Win32Resource />
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <SignAssembly>false</SignAssembly>
    <Authors>G. Gordon Nasseri</Authors>
    <Company>ProphetsWay</Company>
    <Description>
      A small library that is useful when utilizing iBatisNet for a Data Access Layer (DAL).

      Go to https://github.com/ProphetManX/ProphetsWay.iBatisTools for more information.
    </Description>

    <!-- Assembly Override Properties -->
    <PackageRequireLicenseAcceptance />
    <PackageId />
    <Version />
    <Authors />
    <Company />
    <Product />

    <Copyright />
    <PackageLicenseUrl />
    <PackageProjectUrl />
    <PackageIconUrl />
    <RepositoryUrl />
    <RepositoryType />
    <PackageTags />
    <PackageReleaseNotes />
    <NeutralLanguage />
    <AssemblyVersion />
    <FileVersion />
    <InformationalVersion />

  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="IBatisNet" Version="1.0.0" />
    <PackageReference Include="ProphetsWay.BaseDataAccess" Version="1.0.0" />
    <PackageReference Include="ProphetsWay.Utilities" Version="1.0.0" />
    <PackageReference Include="ProphetsWay.Logger" Version="2.0.0" />
  </ItemGroup>

</Project>
